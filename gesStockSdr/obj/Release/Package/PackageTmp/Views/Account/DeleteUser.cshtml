@model gesStockSdr.Models.GesUserViewModel

@{
    ViewBag.Title = "DeleteUser";
    Layout = "~/Views/Shared/LayoutAdmin.cshtml";
}

<h2>Supprimer</h2>

<h3>
    vous voulez vraiment supprimer cet element?
</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class=" control-label"><i class="fas  fa-user"> </i> Utilisateur</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>



        <dt>
            <label class=" control-label"> <i class="fas  fa-key"> </i> Role</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.role)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <input type="submit" value="Supprimer" class="btn btn-danger" />
            <a href="@Url.Action("indexUser", "Account" )" class="btn btn-primary">Annuler</a>

        </div>
        
    }
</div>
